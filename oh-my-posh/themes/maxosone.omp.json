{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#F550AE",
          "foreground": "#FFFFFF",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \u2665 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#A161D3",
          "foreground": "#FFFFFF",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#7A6AE4",
          "foreground": "#FFFFFF",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#5E70F1",
          "foreground": "#FFFFFF",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \uf07b {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#3E77FF",
          "foreground": "#FFFFFF",
          "invert_powerline": true,
          "style": "diamond",
          "template": " \uf489 {{ .Name }} ",
          "trailing_diamond": "\ue0b4",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#29315A",
          "foreground": "#00C7FC",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " \ue285 ",
          "trailing_diamond": "\ue0b0",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
